{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [true, "attribute", "app", "camelCase"],
    "component-selector": [true, "element", "app", "kebab-case"],
    "no-trailing-whitespace": false,
    "typedef-whitespace": false,
    "semicolon": false,
    "one-line": false,
    "no-debugger": false
  }
}
