  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "local-network": "ng serve --host=0.0.0.0",
    "build": "ng build --prod --base-href https://erayismail.online",
    "build-final": "ng build --prod --base-href https://Ralomex.com",
    "cleanFolders": "rm -rf dist && rm -rf docs",
